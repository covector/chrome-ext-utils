{
    "name": "Google Image Left",
    "description": "Align Google image search preview to the left instead of the right.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "logo/icon_16.png",
        "32": "logo/icon_32.png",
        "48": "logo/icon_48.png",
        "128": "logo/icon_128.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {},
    "background": {
        "service_worker": "background.js"
    }
}